  "identifier": "SB253",
  "title": "RELATING TO CONDOMINIUM RESERVES.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Boards of Directors",
    "Budget Summaries",
    "Burden of Proof",
