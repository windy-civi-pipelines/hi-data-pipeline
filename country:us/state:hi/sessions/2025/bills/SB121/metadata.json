  "abstracts": [
    {
      "note": "description",
      "abstract": "Proposes an amendment to the Hawaii State Constitution to extend the amount of time allowed the Senate to consider and act on the appointments of justices and judges from thirty to sixty days for appointments made between April 1 and December 31 when the Senate is not in Regular Session or is about to adjourn the Regular Session."
    }
  ],
