{
  "legislative_session": "2025",
  "identifier": "HB1099",
  "title": "MAKING AN EMERGENCY APPROPRIATION TO THE DEPARTMENT OF HUMAN SERVICES.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Department of Human Services",
    "Emergency Appropriation ($)",
    "Expenditure Ceiling",
    "Supplemental Nutrition Assistance Program"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Provides an emergency appropriation for fifty per cent of the penalty assessed by the United States Department of Agriculture Food and Nutrition Service to be reinvested in the continued development of a new eligibility system that will assist in reducing the State's Supplemental Nutrition Assistance Program payment error rate, which, if successful, may result in the State not being required to pay the remaining fifty per cent of the penalty.  Declares that the general fund expenditure ceiling is exceeded.  (CD1)"
    }
  ],
