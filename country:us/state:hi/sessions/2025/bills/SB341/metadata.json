  "subject": [
    "Interest Owed",
    "Public Employees",
    "Reimbursement",
    "Travel Costs"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires the State or a county to reimburse an officer or employee within thirty calendar days of a request for reimbursement for travel costs that the officer or employee loaned to the State or a county.  Requires the State or a county to pay a percentage of interest on the total amount owed to an officer or employee until the loan is fully reimbursed."
    }
  ],
