  "identifier": "HB786",
  "title": "RELATING TO DEPOSIT BEVERAGE CONTAINER RECYCLING.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Beverage Dealers",
    "Department of Health",
    "Redemption Centers",
