  "legislative_session": "2025",
  "identifier": "SB228",
  "title": "RELATING TO EXCITED DELIRIUM.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Cause of Death",
    "Excited Delirium",
    "Hawaii Rules of Evidence",
    "Incident Report",
    "Law Enforcement Officers",
    "Medical Diagnosis"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Prohibits excited delirium from being recognized as a valid medical diagnosis or cause of death in the State.  Prohibits a medical examiner, coroner, or health care provider from stating on a certificate of death or in any report that the cause of death was excited delirium.  Prohibits law enforcement officers from using the term excited delirium to describe an individual in an incident report.  Establishes a new Hawaii Rule of Evidence that deems evidence that a person experienced or suffered an excited delirium inadmissible in a civil action.  Effective 7/1/3000.  (HD2)"
    }
  ],
