  "subject": [
    "CCFFH",
    "Certified Nurse Aide",
    "DOH",
    "Licensed Nurse",
    "Primary Caregiver Requirement"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires the Department of Health to adopt by rules, a mandate that the primary caregiver caring for two or three clients in community care foster family homes to be a certified nurse aide or licensed nurse who has completed a state-approved training program and other training as required by the Department and holds a current certified nurse aid certificate or nursing license."
    }
  ],
