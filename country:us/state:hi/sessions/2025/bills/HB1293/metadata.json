  "abstracts": [
    {
      "note": "description",
      "abstract": "Exempts Department of Education purchases of local edible produce and packaged food products less than $250,000 from the electronic procurement system requirement.  Requires at least three written quotes for Department of Education purchases of local edible produce and packaged food products of less than $250,000.  Sunsets 6/30/2028.  (CD1)"
    }
  ],
