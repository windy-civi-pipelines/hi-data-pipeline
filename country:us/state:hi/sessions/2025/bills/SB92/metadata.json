  "subject": [
    "Appropriations ($)",
    "Automated External Defibrillators",
    "Cardiac Emergency Response Plan",
    "Cardiopulmonary Resuscitation",
    "Counties",
    "DAGS",
    "Public Schools",
    "State Buildings",
