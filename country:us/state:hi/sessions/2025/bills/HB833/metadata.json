  "identifier": "HB833",
  "title": "RELATING TO COMMUNITY LAND TRUSTS.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Affordable Housing",
    "Appropriations ($)",
    "Community Land Trust Equity Pilot Program",
    "DURF",
    "HHFDC",
    "Reports"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Authorizes the Hawaii Housing Finance and Development Corporation to establish a five-year Community Land Trust Equity Pilot Program to provide community land trusts with a line of credit to fund the acquisition, rehabilitation, renovation, or construction of housing for certain households.  Requires reports to the Legislature regarding the pilot program.  Appropriates funds from the Dwelling Unit Revolving Fund for establishment of the pilot program.  Sunsets the pilot program on 6/30/2030.  Takes effect 7/1/2050.  (SD1)"
    }
  ],
