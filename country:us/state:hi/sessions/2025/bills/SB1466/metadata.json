{
  "legislative_session": "2025",
  "identifier": "SB1466",
  "title": "RELATING TO THE EARNED INCOME TAX CREDIT.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Earned Income Tax Credit",
    "Income Tax"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Clarifies that nonresidents who claim the refundable earned income tax credit are required to adjust the amount claimed using the ratio of their Hawaii adjusted gross income to federal adjusted gross income.  Eliminates the unlimited carryforward on nonrefundable earned income tax credits for tax year 2022."
    }
  ],
