  "legislative_session": "2025",
  "identifier": "HB54",
  "title": "RELATING TO MOTOR VEHICLES.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Excessive Speeding",
    "Penalties",
    "Property Forfeiture",
    "Vehicle"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Increases penalties for a third or subsequent offense within five years of excessive speeding to a misdemeanor.  Increases penalties for a second offense within five years of excessive speeding. Authorizes the court, as part of the person's sentencing for the third or subsequent offense, to order that the vehicle used by the person in the commission of the offense be subject to forfeiture.  Requires individuals convicted of excessive speeding to be subject to identification processing procedures.  Effective 7/1/3000.  (SD2)"
    }
  ],
