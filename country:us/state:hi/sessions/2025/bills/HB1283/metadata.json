  "abstracts": [
    {
      "note": "description",
      "abstract": "Establishes an exemption from mediation in paternity proceedings where there are allegations of domestic abuse.  Clarifies the exemption from mediation in divorce proceedings as it relates to domestic abuse.  Effective 7/1/3000.  (HD2)"
    }
  ],
