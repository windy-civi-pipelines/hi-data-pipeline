  "abstracts": [
    {
      "note": "description",
      "abstract": "Removes from the definition of \"public lands\" lands set aside by the Governor to the counties for the purpose of affordable housing.  Specifies that lands set aside by the Governor to the counties for affordable housing require legislative approval for the sale or gift of such lands.  Effective 7/1/3000.  (HD2)"
    }
  ],
