{
  "legislative_session": "2025",
  "identifier": "SB176",
  "title": "RELATING TO RECOUNTS.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Elections",
    "Initial Tabulation",
    "Recounts"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Allows for ballots designated by the county clerks for inclusion to be included in the initial tabulation for purposes of initial recounts.  Prohibits ballots that the county clerks initially determine are deficient or need additional time to be corrected or verified from being included in the initial tabulation.  Increases the minimum threshold required to trigger an automatic recount.  (CD1)"
    }
  ],
