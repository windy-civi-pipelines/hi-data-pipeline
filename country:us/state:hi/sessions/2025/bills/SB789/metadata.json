  "abstracts": [
    {
      "note": "description",
      "abstract": "Reduces the amount the Department of Education is authorized to charge for school meals to all students enrolled in department schools to no more than one-quarter of the cost of preparing the meals.  Effective 7/1/3000.  (HD1)"
    }
  ],
