  "subject": [
    "Electric Bicycles",
    "Operator Requirements"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Prohibits a person under the age of fifteen from operating a class 3 electric bicycle.  Prohibits a person sixteen years of age or older from operating a class 3 electric bicycle unless the person satisfies certain requirements.  Effective 7/1/3000.  (HD1)"
    }
  ],
