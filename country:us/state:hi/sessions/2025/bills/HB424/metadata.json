  "identifier": "HB424",
  "title": "RELATING TO SCHOOL MEALS.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Appropriation ($)",
    "Cost",
    "Department of Education",
    "Free School Breakfasts and Lunches"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Beginning with the 2025-2026 school year, requires the Department of Education to provide free breakfast and lunch to students enrolled in a public school who meet certain requirements.  Reduces the amount the Department of Education is authorized to charge for school meals to all students enrolled in department schools to no more than an unspecified portion of the cost of preparing the meals.  Appropriates funds.  Effective 7/1/3000.  (HD2)"
    }
  ],
