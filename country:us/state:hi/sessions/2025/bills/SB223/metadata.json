{
  "legislative_session": "2025",
  "identifier": "SB223",
  "title": "RELATING TO FIRE PREVENTION.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Appropriations ($)",
    "Budget",
    "Community Fuels Reduction Project",
    "DLNR",
    "Fire Prevention",
    "Forestry and Wildlife",
