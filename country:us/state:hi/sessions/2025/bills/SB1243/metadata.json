  "abstracts": [
    {
      "note": "description",
      "abstract": "Allows foreign medical school graduates applying for a license to practice medicine and surgery to substitute one year of required medical training or experience in a hospital with one year of fellowship training where the parent program or hospital is accredited by the Accreditation Council for Graduate Medical Education.  Effective 12/31/2050.  (SD1)"
    }
  ],
