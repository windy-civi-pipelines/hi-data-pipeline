  "identifier": "HB827",
  "title": "RELATING TO WORKERS' COMPENSATION MEDICAL BENEFITS.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Cancer",
    "Firefighters",
    "Workers' Compensation Medical Benefits"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Expands workers' compensation medical benefits for firefighters to include coverage for adenocarcinoma or mesothelioma of the respiratory system; cancer of the buccal cavity, colon, pharynx, and thyroid; and malignant melanoma.  Specifies that an employer may prove that the cancer or disease was not associated with the firefighting duties to deny medical benefits under workers' compensation law.  Effective 7/1/3000.  (HD1)"
    }
  ],
