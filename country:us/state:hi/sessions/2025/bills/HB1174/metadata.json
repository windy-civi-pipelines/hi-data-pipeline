  "identifier": "HB1174",
  "title": "RELATING TO THE PROCEDURE FOR PAYMENT UNDER PROTEST LAWSUITS.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Payments Under Protest",
    "Procedure"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Allows for the interest earned on payments under protest in the litigated claims fund to be paid in nontaxation cases if the claimant prevails.  Establishes a procedure for the disposition of moneys and refiling of actions when a payment under protest suit is filed prematurely.  (SD1)"
    }
  ],
