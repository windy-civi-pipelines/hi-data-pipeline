{
  "legislative_session": "2025",
  "identifier": "HB966",
  "title": "RELATING TO AGRICULTURAL TOURISM.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Agricultural Tourism Activities",
    "LUC",
    "Uniform Statewide Standards"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Establishes statewide, uniform standards to promote agricultural tourism activities in the State for all counties that have adopted an agricultural tourism ordinance.  Requires agricultural tourism activities to be registered by the county planning department.  Requires agricultural tourism activities to coexist with an agricultural activity on a farming operation and requires termination of the agricultural tourism activities upon cessation of the agricultural activity, except under certain circumstances.  Makes conforming amendments.  Effective 7/1/3000.  (HD2)"
    }
  ],
