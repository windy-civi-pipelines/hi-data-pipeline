{
  "legislative_session": "2025",
  "identifier": "HB1378",
  "title": "RELATING TO THE STATE FOUNDATION ON CULTURE AND THE ARTS.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Appropriations ($)",
    "Performing Arts Grants Program",
    "Performing Arts Grants Special Fund",
    "SFCA",
    "State Art Museum",
    "Works of Art Special Fund"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Establishes the Performing Arts Grants Program and Performing Arts Grants Special Fund to support performing arts in the State.  Clarifies the scope of the SFCA's responsibilities to include the coordination, planning, promotion, marketing, and execution of performing arts events.  Amends the funding sources for the State Art Museum.  Amends the amounts required to be transferred into, including an annual transfer amount and the allowable expenditures from, the Works of Art Special Fund.  Appropriates funds.  (CD1)"
    }
  ],
