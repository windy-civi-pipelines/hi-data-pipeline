{
  "legislative_session": "2025",
  "identifier": "SB1298",
  "title": "RELATING TO RECYCLING.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Collection",
    "DOH",
    "Electronic Devices",
    "Legacy Devices",
    "Manufacturers",
    "Peripherals",
