  "legislative_session": "2025",
  "identifier": "HB423",
  "title": "RELATING TO WORKERS' COMPENSATION.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Automatic Approval",
    "Certified Providers",
    "Vocational Rehabilitation Services",
    "Workers' Compensation"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Clarifies the process for selecting a certified provider of rehabilitation services.  Requires providers to automatically approve vocational rehabilitation services for an injured employee if those services will likely be required for suitable gainful employment.  Requires providers to file an employee's vocational plan no later than ninety days after submitting an initial evaluation report, with certain exceptions.  Effective 7/1/2050.  (SD1)"
    }
  ],
