  "identifier": "SB528",
  "title": "RELATING TO MARRIAGE OF MINORS.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Legal Age",
    "Marriage"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Raises the minimum age to enter into marriage from sixteen to eighteen years of age.  Repeals the authority of parents and the family court to consent to a minor's marriage.  Repeals spousal cohabitation after the parties attain legal age as an exception for an annulment based on nonage.  Makes conforming amendments."
    }
  ],
