  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires a bidder and their subcontractors for certain public works construction contracts to utilize apprentices if the bidder receives a bidding advantage for being a party to an apprenticeship agreement registered with the State at the time of general bidding and is awarded the contract.  Effective 7/1/3000.  (HD1)"
    }
  ],
