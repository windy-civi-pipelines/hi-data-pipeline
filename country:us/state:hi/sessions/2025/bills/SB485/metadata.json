  "identifier": "SB485",
  "title": "RELATING TO MOTOR VEHICLES.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Exemption",
    "Motor Vehicle Sun Screening Devices",
    "Motor Vehicles Regulations",
    "Number Plates"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Decreases the exempted light transmittance for motor vehicle sun screening devices on front side wing vents and windows from thirty-five per cent to twenty per cent plus or minus six per cent.  Exempts the light transmittance for motor vehicle sun screening devices for side windows that are to the rear of the driver and rear windows in sedans.  Repeals the requirement that a front number plate be affixed to the front of a motor vehicle.  Makes conforming amendments.  Effective 7/1/2050.  (SD1)"
    }
  ],
