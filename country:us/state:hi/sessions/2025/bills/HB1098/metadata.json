{
  "legislative_session": "2025",
  "identifier": "HB1098",
  "title": "RELATING TO CRIMES AGAINST PROTECTIVE SERVICES WORKERS.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Department of Human Services",
    "Physical Harm",
    "Protective Services Workers",
