  "subject": [
    "DOH",
    "Health Care",
    "Organized Ambulatory Health Care Facility",
    "Physicians",
    "Report"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Clarifies that a physician may practice at an organized ambulatory health care facility even if the physician does not have hospital privileges at a licensed hospital in the same geographic location as the ambulatory facility.  Applies to counties with populations of less than five hundred thousand.  Requires the Department of Health to report to the Legislature.  Sunsets 6/30/2028.  (CD1)"
    }
  ],
