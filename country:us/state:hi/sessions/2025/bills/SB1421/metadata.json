  "legislative_session": "2025",
  "identifier": "SB1421",
  "title": "RELATING TO MEDICAL RECORDS.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Ceasing Operations",
    "DOH",
    "Health Care Providers",
    "Repeal",
    "Retention and Preservation of Medical Records"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Repeals the requirement for the Department of Health to approve a health care provider's arrangement for the retention and preservation of patients' medical records before the provider ceases operations.  Clarifies that retention and preservation of medical records must be consistent with federal and state regulations and requires providers to notify patients on how to access or request copies of their medical records.  Effective 7/1/3000.  (HD2)"
    }
  ],
