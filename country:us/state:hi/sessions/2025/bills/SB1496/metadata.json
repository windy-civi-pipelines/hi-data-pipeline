  "legislative_session": "2025",
  "identifier": "SB1496",
  "title": "RELATING TO CIVIL RIGHTS.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Discrimination in Public Accommodations",
    "Discriminatory Practices",
    "Persons with Disabilities",
    "Places of Public Accommodation"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Makes it an unlawful discriminatory practice for places of public accommodation to deny a person with a disability full and equal enjoyment of, or information related to, their goods, services, facilities, privileges, advantages, or accommodations using information and communication technology intended for use by the general public as applicants, participants, customers, clients, or visitors.  Establishes exceptions.  Clarifies that a facility is considered a place of public accommodation regardless of whether its presence in the State is physical or digital.  Effective 7/1/3000.  (HD2)"
    }
  ],
