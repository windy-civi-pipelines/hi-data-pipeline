  "subject": [
    "Bid Submissions",
    "Immaterial or Technical Information",
    "Procurement"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Allows bidders to clarify or correct immaterial or technical information required as part of a bid submission for up to twenty-four hours after the bid submission deadline.  Requires that bids for construction be opened no sooner than twenty-four hours after the deadline for the submission of bids. Defines \"immaterial or technical information\".  Repeals 6/30/2027."
    }
  ],
