  "identifier": "SB701",
  "title": "RELATING TO RENEWABLE ENERGY.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Building Permits",
    "Online Automated Permitting Platform",
    "Self-Certification Process",
    "Solar Distributed Energy Resource Systems"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires government entities in the State that issue building permits to implement SolarAPP+ or a functionally equivalent online automated permitting platform that verifies code compliance and issues permits to licensed contractors for solar distributed energy resource systems in real-time by 1/1/2026.  Requires government entities in the State that issue building permits in areas served by an investor-owned electric utility to adopt a self-certification process for solar distributed energy resource systems that are not SolarAPP+ compatible."
    }
  ],
