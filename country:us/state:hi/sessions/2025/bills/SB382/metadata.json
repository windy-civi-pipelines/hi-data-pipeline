  "identifier": "SB382",
  "title": "RELATING TO PROCUREMENT.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Debriefing",
    "Disclosure of Competing Proposals",
    "Hawaii Public Procurement Code",
    "Protest"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Prohibits a procurement officer from disclosing a competing offeror's proposal or evaluation score, except the summary of scores, during a debriefing requested by a non-selected offeror.  Authorizes the disclosure of a competing offeror's proposal or evaluation score after any protest is resolved and the contract is executed.  Effective 7/1/3000.  (HD1)"
    }
  ],
