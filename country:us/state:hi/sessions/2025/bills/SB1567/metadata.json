{
  "legislative_session": "2025",
  "identifier": "SB1567",
  "title": "RELATING TO THE CLASSIFICATION AND COMPENSATION SYSTEMS.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Civil Service Positions",
    "Classification and Compensation Systems",
    "Comprehensive Review",
    "DHRD",
    "Reports"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires the Department of Human Resources Development to complete a comprehensive review of the classification and compensation systems for all civil service positions under its jurisdiction by 10/31/2026.  Allows the Department to contract with a third party, in accordance with the Hawaii Public Procurement Code, to assist with the timely completion of the review.  Requires reports to the Legislature.  (CD1)"
    }
  ],
