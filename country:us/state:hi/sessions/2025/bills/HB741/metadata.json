  "abstracts": [
    {
      "note": "description",
      "abstract": "Exempts affordable housing projects that are financed by a nonprofit community development financial institution certified by the Hawaii Housing Finance and Development Corporation as utilizing less than a certain amount from the Affordable Homeownership Revolving Fund from prevailing wage provisions.  Effective 7/1/3000.  (HD2)"
    }
  ],
