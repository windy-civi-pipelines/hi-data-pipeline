{
  "legislative_session": "2025",
  "identifier": "HB422",
  "title": "RELATING TO SCHOOL IMPACT FEES.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Construction Cost Component",
    "DOE",
    "Exemptions",
    "Land Component Impact Fee",
    "Report",
    "Rules",
    "School Facilities Authority",
    "School Impact Fees"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Repeals the construction cost component of school impact fees.  Exempts government housing developments, affordable housing units and projects, and Hawaiian home lands housing from school impact fees.  Requires the School Facilities Authority to adopt rules governing fee collection.  Increases the minimum number of units in a development to trigger land dedication provisions of the land component impact fee.  Requires a report to the Legislature on program efficacy.  Sunsets 7/1/2029.  (CD1)"
    }
  ],
