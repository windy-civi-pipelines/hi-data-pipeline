  "subject": [
    "DLIR",
    "Hospitals",
    "Registered Nurses",
    "Staffing Requirements ($)"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Establishes certain minimum registered nurse-to-patient staffing requirements for hospitals.  No later than 9/1/2025, requires hospitals to create hospital registered nurse staffing committees.  Beginning 7/1/2026, requires hospitals to implement registered nurse staffing plans.  Appropriates funds."
    }
  ],
