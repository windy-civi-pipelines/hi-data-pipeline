  "identifier": "SB716",
  "title": "RELATING TO HAWAII EMPLOYMENT SECURITY LAW.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "DLIR",
    "Deregulation",
