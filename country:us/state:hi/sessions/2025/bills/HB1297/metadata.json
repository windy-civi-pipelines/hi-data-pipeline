  "abstracts": [
    {
      "note": "description",
      "abstract": "Sets a time limit for submitting bid protests.  Makes the seventy-five-day limit for issuing a written decision on protests apply to any contract awarded by competitive sealed bid or competitive sealed proposal.  Caps the protest bond amount.  Effective 7/1/3000.  (SD1)"
    }
