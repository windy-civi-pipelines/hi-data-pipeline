  "identifier": "HB256",
  "title": "RELATING TO ENVIRONMENTAL PROTECTION.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Air Pollution Control Permit",
    "Emissions",
    "Standards",
    "Waste-to-energy Facility"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires an owner or operator of a waste-to-energy facility to comply with certain United States Environmental Protection Agency regulations for emission limitations as they existed on 12/31/2024, unless emission limitations are made more stringent by federal regulations or by the state air pollution control permit issued to that owner or operator by the Department of Health.  Sunsets 6/30/2027.  Effective 12/31/2050.  (SD1)"
    }
  ],
