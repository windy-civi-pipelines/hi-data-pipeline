  "identifier": "SB554",
  "title": "RELATING TO WORKERS' COMPENSATION.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Emergency Medical Attendants",
    "Firefighters",
    "First Responders",
