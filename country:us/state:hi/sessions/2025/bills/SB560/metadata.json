  "legislative_session": "2025",
  "identifier": "SB560",
  "title": "RELATING TO MARRIAGE OF MINORS.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Legal Age",
    "Marriage"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Raises the minimum age requirement to enter into marriage from sixteen to eighteen years of age.  Removes the parental consent and written approval by the family court requirements for a minor to marry.  Removes spousal cohabitation after the parties attain legal age as an exception for an annulment based on nonage.  Makes conforming amendments."
    }
  ],
