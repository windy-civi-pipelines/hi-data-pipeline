{
  "legislative_session": "2025",
  "identifier": "HB1162",
  "title": "RELATING TO MOTORCYCLE INSTRUCTION PERMITS.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Basic Motorcycle Rider Course",
    "DOT",
    "Enclosed State Certified Training Range",
    "Motorcycle Instruction Permits"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires all applicants for a motorcycle instruction permit to successfully complete a basic motorcycle rider course approved by the Department of Transportation.  Clarifies that a person shall not operate a motor vehicle in connection with the driving training course without a valid temporary instruction permit unless it is a motorcycle that is being used for training purposes on an enclosed state certified training range.  (CD1)"
    }
  ],
