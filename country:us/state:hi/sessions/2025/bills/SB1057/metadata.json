  "identifier": "SB1057",
  "title": "RELATING TO PROCUREMENT.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Apprenticeable Trade",
    "Apprenticeship Agreement",
    "Procurement",
    "Public Works Construction",
    "Subcontractors",
    "Utilization"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Clarifies that the bid incentive given to bidders for public works construction projects who are parties to an apprenticeship agreement is based on apprenticeable trades utilized, rather than employed, to construct the public works.  Requires the bidder and the bidder's subcontractors to be parties to all apprenticeable trades utilized to construct the project for which a state-approved apprenticeship program exists.  Effective 7/1/2050.  (SD2)"
    }
  ],
