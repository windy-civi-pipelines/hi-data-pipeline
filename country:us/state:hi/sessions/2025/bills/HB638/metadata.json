  "identifier": "HB638",
  "title": "RELATING TO THE RESIDENTIAL LANDLORD-TENANT CODE.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Certified Copies",
    "Credit Report",
    "Criminal Background Check",
    "Rental Application Screening Fee",
    "Reusable Tenant Screening Report"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Prohibits a landlord or the landlord's agent from charging an application fee for a criminal background check or credit report if an applicant provides a certified copy of a criminal background check or credit report.  Prohibits a landlord or the landlord's agent from charging an application fee if a comprehensive reusable tenant screening report is available.  Requires a landlord or the landlord's agent to provide, upon request, a certified copy of an applicant's criminal background check or credit report."
    }
  ],
