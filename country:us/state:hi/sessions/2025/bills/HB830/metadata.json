{
  "legislative_session": "2025",
  "identifier": "HB830",
  "title": "RELATING TO HISTORIC PRESERVATION REVIEWS.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "DLNR",
    "Historic Preservation Review",
    "Mixed-Use Development",
    "Residential Units",
    "Review and Comment",
    "SHPD",
    "Third-Party Consultants ($)"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Beginning 7/1/2026, authorizes the State Historic Preservation Division to contract its review of proposed state projects and projects affecting historic properties to third-party consultants under certain circumstances.  Establishes requirements for qualified third-party consultants providing review services.  Requires the project proponent to pay the reasonable fee requirements of the third-party consultant.  Requires the Department of Land and Natural Resources to publish a draft of its proposed rules and later present its proposed final rules to the Board of Land and Natural Resources.  Sunsets 6/30/2030.  (CD1)"
    }
  ],
