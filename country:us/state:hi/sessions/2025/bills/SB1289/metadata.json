  "subject": [
    "Appropriations ($)",
    "Moving Violations",
    "Traffic Abstract"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Limits moving violations arising from the operation of a motor vehicle that appear on traffic abstracts to those that resulted in convictions.  Requires the Traffic and Emergency Period Violations Bureaus to remove all dismissed moving violations not resulting in a conviction from certified abstracts.  Appropriates funds.  Effective 7/1/2050.  (SD1)"
    }
  ],
