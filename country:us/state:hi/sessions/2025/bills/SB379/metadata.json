  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires that Hawaii Housing Finance and Development Corporation housing projects include a restrictive covenant that states that the units designated as affordable housing, as described in the submitted project application, shall remain as affordable housing in perpetuity.  Prohibits development of affordable housing in a special flood hazard area."
    }
  ],
