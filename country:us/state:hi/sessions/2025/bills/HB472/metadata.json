{
  "legislative_session": "2025",
  "identifier": "HB472",
  "title": "RELATING TO DIGITAL IDENTIFICATION.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Digitized Identification Cards",
    "Valid Identification"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Allows digitized identification cards to be accepted as a valid form of identification, under certain conditions.  Authorizes state and county law enforcement agencies to accept a digitized identification card as proof of a person's identity beginning 7/1/2027.  Effective 7/1/2077.  (SD2)"
    }
  ],
