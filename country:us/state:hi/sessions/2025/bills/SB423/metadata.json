{
  "legislative_session": "2025",
  "identifier": "SB423",
  "title": "RELATING TO THE EARLY LEARNING BOARD.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Composition",
    "Early Learning Board",
    "Hawaii Head Start State Collaboration Office",
    "Head Start Association of Hawaii",
    "Membership"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires one voting member of the Early Learning Board to have experience as a local provider of early childhood education and development services.  Adds the Director of the Hawaii Head Start State Collaboration Office as an ex officio, nonvoting member to the Early Learning Board.  Requires the Early Learning Board to invite the President of the Head Start Association of Hawaii, or their designee, to serve as an ex officio, nonvoting member of the Board.  (CD1)"
    }
  ],
