  "legislative_session": "2025",
  "identifier": "HB945",
  "title": "RELATING TO HOTELS.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Hotel",
    "Notification",
    "Service Disruptions"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires a hotel to provide notification of a service disruption, or that a service disruption may occur, to all third-party vendors and prospective and current guests.  Prohibits a hotel from imposing any fee, penalty, or other charge, or retaining any deposit:  if before check-in, a guest cancels a reservation due to a service disruption, unless the hotel provided prior notice; or if the service disruption arises after check-in, regardless of whether the hotel provided prior notification."
    }
  ],
