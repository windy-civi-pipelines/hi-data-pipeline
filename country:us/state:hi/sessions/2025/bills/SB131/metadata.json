  "identifier": "SB131",
  "title": "RELATING TO PROCUREMENT.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Penalties for NonPerformance or Major Delay",
    "Procurement",
    "Procurement Policy Board"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires the Procurement Policy Board to adopt rules requiring contracts to impose mandatory fines or provide for termination of a contract for default if a contractor fails to perform the contractor's duties or if there is a major delay in a deliverable.  Requires each contract to establish a schedule of deliverables and to specify what constitutes a \"major delay\"."
    }
  ],
