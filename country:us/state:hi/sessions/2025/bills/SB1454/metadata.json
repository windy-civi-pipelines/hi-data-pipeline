{
  "legislative_session": "2025",
  "identifier": "SB1454",
  "title": "RELATING TO THE WAGE AND HOUR LAW.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "DLIR",
    "Enforcement",
    "Order of Wage Payment Violation",
    "Penalties",
    "Wage and Hour Law"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires the Department of Labor and Industrial Relations to issue an order of wage payment violation to employers found to be in violation of the State's Wage and Hour Law.  Establishes penalties, enforcement, and appeal procedures.  Amends the definition of \"wage\" to clarify that it means compensation for labor or services rendered by an employee, regardless of how it is calculated.  (CD1)"
    }
  ],
