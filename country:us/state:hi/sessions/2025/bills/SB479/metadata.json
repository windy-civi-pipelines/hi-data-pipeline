  "legislative_session": "2025",
  "identifier": "SB479",
  "title": "RELATING TO THE HAWAII ABLE SAVINGS PROGRAM.",
  "from_organization": "~{\"classification\": \"upper\"}",
  "classification": [
    "bill"
  ],
