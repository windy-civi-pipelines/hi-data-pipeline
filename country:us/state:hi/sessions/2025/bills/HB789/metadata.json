  "subject": [
    "Appropriation ($)",
    "DHS",
    "Hawaii Home Energy Assistance Program",
    "PUC"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Establishes the Hawaii Home Energy Assistance Program within the Department of Human Services to assist households in paying their energy bills.  Requires the Public Utilities Commission's Public Benefits Fee Administrator to provide information and technical assistance about available energy efficiency programs to recipients of the program.  Appropriates funds."
    }
