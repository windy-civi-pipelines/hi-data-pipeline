  "abstracts": [
    {
      "note": "description",
      "abstract": "Removes language specifying that an appeal from the Tax Appeal Court be filed with the Tax Appeal Court.  Allows an appeal from the Tax Appeal Court to be filed within thirty days of entry of a final judgment."
    }
  ],
