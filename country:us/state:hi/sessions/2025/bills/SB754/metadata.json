  "subject": [
    "Expiration",
    "Low-Income Housing Tax Credit",
    "Taxation"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Provides that a tax credit for low-income housing that exceeds the taxpayer's income tax liability for a taxable year may be used as a credit against the taxpayer's income tax liability in subsequent taxable years, but not after the tenth subsequent taxable year."
    }
  ],
