{
  "legislative_session": "2025",
  "identifier": "HB1146",
  "title": "RELATING TO PASS-THROUGH ENTITY TAXATION.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Partnerships",
    "Pass-Through Entities",
    "S Corporations",
    "Taxation"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires all qualified members claiming a credit for pass-through entity taxation to adjust their income to include the qualified member's share of taxes paid by an electing pass-through entity for taxable years beginning after December 31, 2024.  (SD1)"
    }
  ],
