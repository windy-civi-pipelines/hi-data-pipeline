  "subject": [
    "Appropriations ($)",
    "Department of Accounting and General Services",
    "Stadium Authority",
    "Stadium Development Special Fund"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Requires conditions placed on appropriations to the Stadium Development Special Fund to be met before expenditure.  Restricts private donations to the Stadium Authority to use on stadium infrastructure and stadium development costs.  Appropriates funds out of the Stadium Development Special Fund, subject to the completion of specific project readiness conditions.  Requires certain moneys expended to be from private sources.  Effective 7/1/2050.  (SD1)"
    }
  ],
