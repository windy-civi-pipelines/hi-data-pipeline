  ],
  "subject": [
    "Agricultural Land",
    "Appropriation ($)",
    "Foreign Entities",
    "Lease Limitations"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Prohibits foreign entities from owning, leasing, or holding a controlling interest in more than an unspecified number of acres of agricultural land.  Limits the lease term for agricultural land by foreign entities.  Requires foreign entities that own or lease interest in agricultural lands to file an annual report with the Department of Agriculture.  Appropriates funds."
    }
  ],
