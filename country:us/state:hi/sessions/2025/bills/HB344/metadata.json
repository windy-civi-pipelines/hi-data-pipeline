  "legislative_session": "2025",
  "identifier": "HB344",
  "title": "RELATING TO ELECTRIC VEHICLE CHARGING INFRASTRUCTURE.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Appropriation ($)",
    "DAGS",
    "DOT",
    "Electric Vehicle Charging Infrastructure",
    "HSEO",
