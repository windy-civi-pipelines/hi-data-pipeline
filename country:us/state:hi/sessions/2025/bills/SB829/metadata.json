  "abstracts": [
    {
      "note": "description",
      "abstract": "Authorizes physicians to practice at organized ambulatory health care facilities even if the physician does not have hospital privileges at a licensed hospital in the same geographic location as the ambulatory facility.  Provides that a written transfer agreement is not required to transfer a patient from an organized ambulatory health care facility to a licensed hospital."
    }
  ],
