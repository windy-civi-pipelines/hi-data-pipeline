{
  "legislative_session": "2025",
  "identifier": "HB969",
  "title": "RELATING TO WASTE OR DISPOSAL FACILITIES.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Agricultural District",
    "Buffer Zones",
    "CWRM",
    "DOH",
    "Landfill Units",
    "Prohibition",
    "Underground Injection Control Lines",
    "Waste Disposal Facilities"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Prohibits landfill units on land in an agricultural district with class A soils in a county with a population greater than five hundred thousand.  Beginning 7/1/2025, narrows the prohibition on constructing, modifying, or expanding waste or disposal facilities within a one-half mile buffer zone of residential, school, or hospital property lines to apply specifically to landfill units or components of landfill units.  Beginning 7/1/2025, prohibits the construction, modification, or expansion of a landfill unit, or any component of a landfill unit, inland of an underground injection control line in a county with a population greater than five hundred thousand, with certain exemptions.  (CD1)"
    }
  ],
