  "subject": [
    "Capital Punishment",
    "Commercial Sexual Exploitation",
    "Sex Trafficking"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Provides for a sentence of death or life imprisonment without possibility of parole upon a conviction for sex trafficking.  Requires a separate sentencing proceeding by a jury, except if waived by the defendant, for a conviction for sex trafficking.  Makes commercial sexual exploitation a class A felony.  Makes conforming amendments."
    }
  ],
