  "identifier": "HB320",
  "title": "RELATING TO SUPPORTED DECISION-MAKING AGREEMENTS.",
  "from_organization": "~{\"classification\": \"lower\"}",
  "classification": [
    "bill"
  ],
  "subject": [
    "Qualified Adults",
    "Restrictions",
    "Supported Decision-Making Agreement",
    "Supportive Community",
    "Terms"
  ],
  "abstracts": [
    {
      "note": "description",
      "abstract": "Allows qualified adults, including adults with a disability, mentally ill adults, and adults sixty-five years of age or older, to enter into supported decision-making agreements with one or more members of a supportive community.  Specifies the terms of a supported decision-making agreement, including access to personal information, agreement requirements, and circumstances for termination.  (CD1)"
    }
  ],
